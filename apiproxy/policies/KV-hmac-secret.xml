<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations continueOnError="false" enabled="true" name="KV-hmac-secret" mapIdentifier="github-webhook-hmac">
  <ExclusiveCache>false</ExclusiveCache>
  <ExpiryTimeInSecs>300</ExpiryTimeInSecs>
  <!-- secret is github-webhook-secret which is the actual value set in KVM -->
  <Get assignTo="private.webhook_secret">
    <Key>
      <Parameter>webhook_secret</Parameter>
    </Key>
  </Get>
  <Scope>apiproxy</Scope>
</KeyValueMapOperations>
